{
  "name": "Connection Forwarder",
  "author": "Kyle Graehl",
  "version": "0.0.2",
  "manifest_version": 2,
  "offline_enabled": true,
  "minimum_chrome_version": "60",
  "app": {
    "background": {
      "scripts": ["common.js","deque.js","promise.filesystem.js","chromise.js","reloader.js","background.js"]
    }
  },
  "permissions": [
    "system.network"
  ],
  "optional_permissions": [
    "background",
    "notifications"
  ],
  "sockets": {
    "udp": {
      "send": [""],
      "bind": [""],
      "multicastMembership": ""
    },
    "tcp": {
      "connect": [""]
    },
    "tcpServer": {
      "listen": ["*:*"]
    }
  }
}
