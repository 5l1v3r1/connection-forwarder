{
  "name": "Connection Forwarder",
  "author": "Kyle Graehl",
  "version": "0.0.3",
  "manifest_version": 2,
  "offline_enabled": true,
  "minimum_chrome_version": "60",
  "app": {
    "background": {
      "scripts": ["js/common.js","js/third-party/deque.js","js/third-party/promise.filesystem.js","js/third-party/chromise.js","js/reloader.js","js/background.js","js/runtime.js"]
    }
  },
  "permissions": [
    "system.network",
    "storage"
  ],
  "optional_permissions": [
    "background",
    "notifications"
  ],
  "sockets": {
    "udp": {
      "send": [""],
      "bind": [""],
      "multicastMembership": ""
    },
    "tcp": {
      "connect": [""]
    },
    "tcpServer": {
      "listen": ["*:*"]
    }
  }
}
